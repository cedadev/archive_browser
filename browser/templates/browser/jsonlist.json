

{"filelist":
  {
      {% for file_rec in file_recs %}
      "{{file_rec.path}}": {
        "href": "{{download_service}}{{file_rec.path}}?download=1",
      	"type": "application/data",
        "role": ["data"],
        "size": {{file_rec.size}},
        "last_modified": "{{file_rec.last_modified}}",
      }{% if not forloop.last %},{% endif %}
      {% endfor %}
  }
}